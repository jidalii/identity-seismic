{
  "name": "seismic-viem",
  "version": "1.0.16",
  "description": "Typescript interface for Seismic",
  "type": "module",
  "main": "dist/_cjs/index.js",
  "module": "dist/_esm/index.ts",
  "types": "dist/_types/index.d.ts",
  "typings": "./_types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/_cjs/index.js",
      "import": "./dist/_esm/index.js",
      "types": "./dist/_types/index.d.ts"
    }
  },
  "files": [
    "dist/_cjs/*.js",
    "dist/_esm/*.js",
    "dist/_types/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit --project tsconfig.types.json",
    "build": "bun build:cjs && bun build:esm && bun build:types",
    "build:cjs": "bun build src/index.ts --outdir dist/_cjs --target browser --format cjs",
    "build:esm": "bun build src/index.ts --outdir dist/_esm --target browser --format esm",
    "build:types": "tsc --project tsconfig.types.json && tsc-alias --project tsconfig.types.json",
    "postbuild": "rimraf ./dist/tsconfig.types.tsbuildinfo",
    "docs:gen": "typedoc src/index.ts --out ../../docs/pages/seismic-viem && bash ../../docs/replace_sources.sh ../../docs/pages/seismic-viem",
    "docs:check": "typedoc src/index.ts --json ./typedoc.out.json; rm ./typedoc.out.json"
  },
  "dependencies": {
    "@noble/hashes": "^1.7.0",
    "@noble/curves": "^1.8.0",
    "@noble/ciphers": "^1.2.0",
    "viem": "^2.21.50"
  },
  "devDependencies": {},
  "license": "MIT",
  "author": "Seismic Systems",
  "homepage": "https://seismic.systems",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeismicSystems/seismic-client"
  },
  "keywords": [
    "seismic",
    "crypto",
    "shielding",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3",
    "typescript"
  ]
}
